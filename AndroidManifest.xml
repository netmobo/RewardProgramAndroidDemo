<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.feefactor.samples.android.rewards"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name"
    		android:name="QuickstartApplication">
		<activity android:label="@string/app_name" android:name="LoginActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:label="@string/app_name" android:name="ClaimRewardActivity"/>
		<activity android:label="@string/app_name" android:name="PurchaseProductActivity"/>
		<activity android:label="@string/app_name" android:name="ViewHistoryActivity"/>
    </application>
   	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
	<uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
</manifest> 